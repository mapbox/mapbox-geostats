{
  "name": "@maptiler/tilestats",
  "version": "1.1.2",
  "description": "Generate statistics about geographic data.",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "tilestats-generate": "bin/tilestats-generate",
    "tilestats-validate": "bin/tilestats-validate"
  },
  "scripts": {
