    "@mapbox/mbtiles": "^0.12.1",
    "@mapbox/tilelive": "^6.1.1",
    "@mapbox/tiletype": "^0.3.0",
    "@mapbox/vector-tile": "^1.2.1",
    "d3-queue": "^3.0.7",
    "jsonschema": "^1.4.1",
    "lodash.isplainobject": "^4.0.6",
    "mapnik": "^4.5.9",
    "meow": "^9.0.0",
    "pbf": "^3.2.1"
